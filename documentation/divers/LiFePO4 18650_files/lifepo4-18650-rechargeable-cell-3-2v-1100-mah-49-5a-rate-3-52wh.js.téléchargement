        }
        
        
    } 
    
    
} 




function IsLight(elem){
    if (typeof(lightback) != 'undefined') return lightback;
    
    return true; 
    //Too many are failing...
    
    if (saJQ(elem).length == 0) return;
    
    var r,b,g,hsp;
    
    var bgColor = "";
    while(saJQ(elem).prop('tagName').toLowerCase() != "html") {
        bgColor = saJQ(elem).css("background-color");
        if(bgColor != "rgba(0, 0, 0, 0)" && bgColor != "transparent")
            break;
        elem = saJQ(elem).parent();
    }
    
    if (bgColor == "transparent") return true;
    
    a = bgColor;

    if (a.match(/^rgb/)) {
        a = a.match(/^rgba?\((\d+),\s*(\d+),\s*(\d+)(?:,\s*(\d+(?:\.\d+)?))?\)$/);
        r = a[1];
        b = a[2];
        g = a[3];
    } else {
        a = +("0x" + a.slice(1).replace( // thanks to jed : http://gist.github.com/983661
        a.length < 5 && /./g, '$&$&'
        )
        );
        r = a >> 16;
        b = a >> 8 & 255;
        g = a & 255;
    }
    hsp = Math.sqrt( // HSP equation from http://alienryderflex.com/hsp.html
        0.299 * (r * r) +
        0.587 * (g * g) +
        0.114 * (b * b)
        );
    return (hsp>127.5);
}



function saOpenPage(page, sort)
{    
    var html = '';
    var start = page * sa_products_count;
    
    for (var i=0; i<sa_products_count; i++) {
        var index = start+i;
        if (index >= sa_foundrows || index >= Object.keys(sa_product_reviews[sort]).length) break;
        
        var review = sa_product_reviews[sort][index];
        if (review['state'] && review['country']) {
            var location = '('+review['state']+', '+review['country']+')';
        }
        else if (review['state']) {
            var location = '('+review['state']+')';
        }
        else if (review['country']) {
            var location = '('+review['country']+')';
        }
        else {
            var location = '';
        }
            
        html += '<div class="product_review">';
        html += '<div class="main-questions">';
        if (typeof(review['product']) != 'undefined' && review['product'] != '')
            html += '<div class="rproduct">'+review['product']+'</div>';
        
        html += '<div class="rating"><div class="stars">'; 
        
        html += saStars(review['rating']);
        
        html += '</div><div class="images">';
        if (review['verified']) {        
            html += '<div class="verified">Verified Buyer</div>';
        }
        if (review['recommend']) {
            html += '<div class="recommend">' + review['recommend'] + '</div>';
        }
        
        html += '</div></div>';
        if (review['heading'] != '')
            html += '<div class="rheading">'+review['heading']+'</div>';
        html += '<div class="info">'+formatDate(review['date'], sa_date_format)+' by '
                + '<span class="name">'+review['name']+'</span>';
                
        if (review['info']) {
            html += '<span class="extrainfo">';
            for (var infoi=0; infoi<review['info'].length; infoi++) {
                html += ', ' + review['info'][infoi];
            }
            html += '</span>';
        }
                
        html += ' ' + location + '</div>'
            + '<div class="comments">&ldquo;'+review['comments']+'&rdquo;</div>';
        
        html += '</div>';
            
        if (review['custom']) {
            html += '<div class="custom-questions">';
            for (var question in review['custom']) {
                html += '<div class="custom-question">' + question + '</div><div class="custom-answer">' + review['custom'][question] + '</div>';
            }
            html += '</div>';
        }  
        
        if (review['media']) {
            html += '<div class="sa_media">';
            for (var j=0, end = review['media'].length; j<end; j++) {
                html += '<img src="//www.shopperapproved.com/widgets/product-image.php?mediaid='+review['media'][j]+'" />';
            }
            html += '</div>';
        }
        
        if (review['resolution']) {
            html += '<div class="resolution-attempt">'+sa_domain+' tried to help this customer via the Shopper Approved '
                + ' Customer Resolution Center, but the customer did not respond to the assistance provided.</div>';
        }  
        if (review['response']) {
            html += '<div class="sa_response_header">Response from '+sa_domain+':</div>'
                + '<div class="sa_response">'+review['response']+'</div>';
        }
        html += '<div class="end_placeholder"></div>';  
             
        html += '</div>';   
    }
    
    html += '<div class="paging">'
    
    if (sa_foundrows > sa_products_count)
    {
    
        if (page > 1) {
            j = page-1;
            html += '<input type="button" class="sa_page arrow" value=" &#9666;    '+j+'" />';
        }
                
        for (var j=page; j<page+3; j++) {
            var activepage = (page == j-1) ? 'active' : '';
            if (j > 0 && (j-1) * sa_products_count < sa_foundrows)
                html += '<input type="button" class="sa_page '+activepage+'" value="'+j+'" />';
        } 
        if (page == 0 && 2 * sa_products_count < sa_foundrows)
            html += '<input type="button" class="sa_page" value="3" />'; 
        if ((page+2) * sa_products_count < sa_foundrows) {
            //j = Math.ceil(sa_foundrows / sa_products_count);
            j = page+3;
            html += '<input type="button" class="sa_page arrow" value=" &#9656;     '+j+'" />';    
        }
        
        
        
    }     
    
    var sortoptions = { 'high':'High to Low', 'new':'Newest to Oldest', 'old':'Oldest to Newest', 'low':'Lowest to Highest'};
    if (sa_rtype == 'merchant' && (typeof(sa_hasfeatured) == null || sa_hasfeatured)) {
        sortoptions['featured'] = 'Featured Reviews';
    }
    html += '<select id="sa_sort"><option value="" disabled style="display: none;">Sort Reviews</option>'; //So it doesn't get caught in yahoo validation
    for (var r in sortoptions) {
        var s = (r == sort) ? 'selected="selected"' : '';
        html += '<option value="'+r+'" '+s+'>'+sortoptions[r]+'</option>';
    }
    html += '</select>';    
    
    
    html += '</div>';
    
    var rnd = Math.random();
    if (typeof(first) != 'undefined') {
        var sa_reviews = saJQ('.sa_review');
    }
    else {
         var sa_reviews = saJQ('.shopperapproved_widget .sa_review');
         if (sa_reviews.length == 0)
            sa_reviews = saJQ('.sa_review'); 
         else
            sa_reviews = saJQ(sa_reviews).not('.sa_static .sa_review');
    } 
    
    saJQ('#product_page').html(html).animate({ opacity: 1 },300, function () {
        
        loadPagingEvents(sort);
        
    }); 
        
}

if (document.URL.indexOf('shopperapproved_watch') > 0 || sa_getCookie('shopperapproved_watch') == 'debug') {
    sa_setCookie('shopperapproved_watch', 'debug');
    saLoadScript('//www.shopperapproved.com/widgets/watch-page.php?siteid='+sa_siteid+'&productid='+sa_productid);
}
else if (document.URL.indexOf('shopperapproved_staging') > 0 || sa_getCookie('shopperapproved_staging') == 'debug') {
    sa_setCookie('shopperapproved_staging', 'debug');
    saLoadScript('//staging.shopperapproved.com/widgets/watch-page.php?siteid='+sa_siteid+'&productid='+sa_productid);
}

initsaJQProductPage();
